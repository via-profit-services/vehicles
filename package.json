{
    "name": "@via-profit-services/vehicles",
    "support": "https://via-profit.ru",
    "version": "1.3.0",
    "description": "Via Profit Vehicles Schema",
    "main": "dist/index.js",
    "engines": {
        "node": ">= 12.8.3",
        "npm": ">= 6.14.8"
    },
    "keywords": [
        "graphql",
        "vehicles",
        "cars",
        "transport"
    ],
    "scripts": {
        "postinstall": "node ./scripts/postinstall.js",
        "make": "node ./scripts/makeMigration.js",
        "split": "node ./scripts/split.js",
        "format": "node ./scripts/format.js",
        "dist": "yarn lint && yarn build:dist && yarn build:migrations",
        "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack-config-dev.ts",
        "build:dist": "webpack --config ./webpack/webpack-config-prod.ts",
        "build:migrations": "webpack --config ./webpack/webpack-config-migrations.ts",
        "start": "yarn build:dev --watch",
        "debug": "cross-env DEBUG=true yarn build:dev --watch",
        "analyze": "cross-env ANALYZE=true yarn build:dist",
        "test": "jest",
        "lint": "eslint --ext .ts ./src/ --fix .",
        "migrate:latest": "yarn knex migrate:latest --knexfile ./.knex/knexfile.js",
        "migrate:rollback": "yarn knex migrate:rollback --knexfile ./.knex/knexfile.js",
        "migrate:rollback:all": "yarn knex migrate:rollback --all --knexfile ./.knex/knexfile.js",
        "migrate:up": "yarn knex migrate:up --knexfile ./.knex/knexfile.js",
        "migrate:down": "yarn knex migrate:down --knexfile ./.knex/knexfile.js",
        "migrate:make": "yarn knex migrate:make --migrations-directory ./src/database/migrations -x ts",
        "seed:run": "yarn knex seed:run --knexfile ./.knex/knexfile.js",
        "seed:run:specific": "yarn knex seed:run --knexfile ./.knex/knexfile.js --specific="
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:via-profit-services/vehicles.git"
    },
    "author": {
        "name": "Via Profit",
        "email": "promo@via-profit.ru",
        "url": "https://dev.via-profit.ru"
    },
    "contributors": [
        "Vaily Novosad <delhsmail@gmail.com>",
        "Pavel Natalin <trubonru@gmail.com>"
    ],
    "license": "MIT",
    "peerDependencies": {
        "@via-profit-services/core": "^1.2.4",
        "@via-profit-services/knex": "^1.1.1",
        "@via-profit-services/redis": "^1.0.3",
        "graphql": "*",
        "ioredis": "^4.27.3",
        "moment": "*",
        "moment-timezone": "*",
        "uuid": "*",
        "winston": "*",
        "winston-daily-rotate-file": "*"
    },
    "devDependencies": {
        "@graphql-tools/schema": "^7.1.3",
        "@jagi/jest-transform-graphql": "^1.0.2",
        "@types/express": "^4.17.11",
        "@types/faker": "^4.1.10",
        "@types/jest": "^24.0.23",
        "@types/node": "^13.13.5",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^7.0.0",
        "@types/webpack": "^4.41.2",
        "@types/webpack-bundle-analyzer": "^3.9.2",
        "@types/webpack-merge": "^4.1.5",
        "@types/webpack-node-externals": "^1.7.0",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "@via-profit-services/core": "^1.2.4",
        "@via-profit-services/dataloader": "^2.0.1",
        "@via-profit-services/eslint-config": "^0.1.11",
        "@via-profit-services/knex": "^1.1.1",
        "@via-profit-services/redis": "^1.0.3",
        "@zerollup/ts-transform-paths": "^1.7.11",
        "chalk": "^4.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^8.1.1",
        "cross-env": "^7.0.3",
        "dotenv": "^8.2.0",
        "eslint": "^7.23.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-import-resolver-node": "^0.3.4",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-promise": "^4.3.1",
        "eslint-plugin-react": "^7.23.1",
        "eslint-plugin-standard": "^5.0.0",
        "express": "^4.17.1",
        "faker": "^5.5.2",
        "form-data": "^4.0.0",
        "geo-tz": "^6.0.1",
        "glob": "^7.1.6",
        "graphql": "^15.5.0",
        "install-peerdeps": "^3.0.3",
        "ioredis": "^4.27.3",
        "jest": "^26.6.3",
        "jest-transform-graphql": "^2.1.0",
        "knex": "^0.95.4",
        "minimist": "^1.2.5",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.33",
        "nodemon": "^2.0.7",
        "nodemon-webpack-plugin": "^4.5.2",
        "pg": "^8.5.1",
        "prettier": "^2.2.1",
        "raw-loader": "^4.0.2",
        "remove-files-webpack-plugin": "^1.4.4",
        "replace": "^1.2.1",
        "rimraf": "^3.0.2",
        "terser-webpack-plugin": "^5.1.1",
        "translation-google": "^0.2.1",
        "ts-jest": "^26.5.4",
        "ts-loader": "^8.1.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4",
        "uuid": "^8.3.2",
        "webpack": "^5.31.0",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-cli": "^4.6.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.7.3",
        "webpack-node-externals": "^2.5.2",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.2"
    }
}
