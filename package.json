{
    "name": "@via-profit-services/vehicles",
    "private": true,
    "support": "https://via-profit.ru",
    "version": "0.1.0",
    "description": "Via Profit Vehicles Schema",
    "main": "dist/index.js",
    "scripts": {
        "make": "node ./scripts/makeMigration.js",
        "split": "node ./scripts/split.js",
        "format": "node ./scripts/format.js",
        "dist": "yarn lint && yarn test && yarn build:dist",
        "start": "yarn start:dev",
        "debug": "yarn build:dev && node --inspect-brk=9229 ./build/playground.js",
        "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.config.dev.js",
        "build:dist": "webpack --config ./webpack/webpack.config.prod.js && yarn make",
        "start:dev": "yarn build:dev --watch",
        "test": "jest",
        "lint": "eslint --fix --ext .ts --ext .js ./src/",
        "via-profit-core:knex": "yarn via-profit-core knex --knexfile src/utils/knexfile.ts",
        "knex:migrate:list": "yarn via-profit-core:knex migrate list",
        "knex:migrate:make": "yarn via-profit-core:knex migrate make --name",
        "knex:migrate:up": "yarn via-profit-core:knex migrate up",
        "knex:migrate:down": "yarn via-profit-core:knex migrate down",
        "knex:migrate:latest": "yarn via-profit-core:knex migrate latest",
        "knex:migrate:rollback": "yarn via-profit-core:knex migrate rollback",
        "knex:migrate:rollback:all": "yarn via-profit-core:knex migrate rollback-all",
        "knex:seed:make": "yarn via-profit-core:knex seed make --name",
        "knex:seed:run": "yarn via-profit-core:knex seed run --name",
        "preversion": "yarn dist && git add --all && git commit -m \"Build\"",
        "postversion:readme": "yarn replace '/vehicles\\.git#semver:\\^\\d+\\.\\d+\\.\\d+' \"/vehicles.git#semver:^$npm_package_version\" ./README.md",
        "postversion": "yarn postversion:readme && git add --all && git commit -m \" Bump README.md\" && git push --tags",
        "release:patch": "yarn version --patch",
        "release:minor": "yarn version --minor",
        "release:major": "yarn version --major",
        "release:prerelease": "yarn version --prerelease"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:via-profit-services/vehicles.git"
    },
    "author": "Via Profit <promo@via-profit.ru>",
    "license": "MIT",
    "dependencies": {
        "@via-profit-services/core": "^0.30.10"
    },
    "devDependencies": {
        "@jagi/jest-transform-graphql": "^1.0.2",
        "@types/faker": "^4.1.10",
        "@types/jest": "^24.0.23",
        "@types/node": "^13.13.5",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^7.0.0",
        "@types/webpack": "^4.41.2",
        "@types/webpack-merge": "^4.1.5",
        "@types/webpack-node-externals": "^1.7.0",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "@via-profit-services/eslint-config": "^0.1.9",
        "@zerollup/ts-transform-paths": "^1.7.11",
        "chalk": "^3.0.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "^6.0.3",
        "dotenv": "^8.2.0",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-config-standard": "^16.0.1",
        "eslint-config-via-profit": "ssh://git@github.com:via-profit/eslint-config-via-profit.git#semver:^0.2.15",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-standard": "^4.0.2",
        "faker": "^4.1.0",
        "filemanager-webpack-plugin": "^2.0.5",
        "form-data": "^3.0.0",
        "geo-tz": "^6.0.0",
        "glob": "^7.1.6",
        "husky": "^4.2.3",
        "install-peerdeps": "^2.0.3",
        "jest": "^24.9.0",
        "jest-transform-graphql": "^2.1.0",
        "lodash": "^4.17.15",
        "minimist": "^1.2.3",
        "nodemon": "^2.0.2",
        "nodemon-webpack-plugin": "^4.2.2",
        "prettier": "^2.1.2",
        "puppeteer": "^4.0.0",
        "remove-files-webpack-plugin": "^1.2.2",
        "replace": "^1.2.0",
        "rimraf": "^3.0.2",
        "supertest": "^4.0.2",
        "terser-webpack-plugin": "^2.3.5",
        "translation-google": "^0.2.1",
        "ts-jest": "^24.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.7.2",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2",
        "webpack-shell-plugin": "^0.5.0"
    }
}
